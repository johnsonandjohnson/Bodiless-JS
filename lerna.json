{
  "packages": [
    "sites/*",
    "packages/*"
  ],
  "command": {
    "publish": {
      "message": "chore(release): publish %s"
    },
    "version": {
      "allowBranch": [
        "main",
        "release"
      ]
    }
  },
  "version": "1.0.0-rc.21",
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "useNx": true,
  "useWorkspaces": false
}
