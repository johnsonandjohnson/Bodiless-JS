  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:lib build:search",
    "build:docs": "bl-docs-build",
    "build:env-vars": "bodiless generate-env-vars",
    "build:lib": "bl-clear-logs && next build && bl-validate-logs",
    "build:search": "create-search-index",
    "build:stats": "BODILESS_BUILD_STATS=1 npm run build",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf .next .edit next-env.d.ts",
    "dev-backend": "bodiless-backend",
    "dev-frontend": "next dev -p ${APP_PORT:-8000}",
    "docs": "run-s build:docs serve:docs",
    "lint": "next lint",
    "prepare": "node ../../scripts/set-missing-symlinks.js @bodiless",
    "prestart": "run-s build:env-vars build:docs",
    "serve": "next start -p ${APP_PORT:-9000}",
    "serve:docs": "docsify serve ./doc",
    "start": "run-p -r dev-frontend dev-backend",
    "edit": "BODILESS_EDIT_MODE=1 npm run build && BODILESS_EDIT_MODE=1 run-p -r serve dev-backend"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "tailwindcss-dir": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.0.0",
    "webpack": "^5.69.1",
    "@next/eslint-plugin-next": "^12.3.4"
  }
