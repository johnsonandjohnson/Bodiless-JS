{
  "name": "--vital--",
  "sideEffects": false,
  "version": "1.0.0-rc.34",
  "description": "Starter template for a Design System package based on Vital DS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "license": "Apache-2.0",
  "private": true,
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js",
    "/tailwind.config.js",
    "/doc",
    "./shadow.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "run-p copy build:lib",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "copy": "copyfiles -u 1 \"./src/**/*.css\" \"./lib/\"",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-rc.34",
    "@bodiless/core": "^1.0.0-rc.34",
    "@bodiless/fclasses": "^1.0.0-rc.34",
    "@bodiless/i18n": "^1.0.0-rc.34",
    "@bodiless/vital-accordion": "^1.0.0-rc.34",
    "@bodiless/vital-buttons": "^1.0.0-rc.34",
    "@bodiless/vital-card": "^1.0.0-rc.34",
    "@bodiless/vital-editors": "^1.0.0-rc.34",
    "@bodiless/vital-elements": "^1.0.0-rc.34",
    "@bodiless/vital-flowcontainer": "^1.0.0-rc.34",
    "@bodiless/vital-image": "^1.0.0-rc.34",
    "@bodiless/vital-layout": "^1.0.0-rc.34",
    "@bodiless/vital-link": "^1.0.0-rc.34",
    "@bodiless/vital-list": "^1.0.0-rc.34",
    "@bodiless/vital-navigation": "^1.0.0-rc.34",
    "@bodiless/vital-search": "^1.0.0-rc.34",
    "@bodiless/vital-table": "^1.0.0-rc.34",
    "@bodiless/vital-templates": "^1.0.0-rc.34",
    "@bodiless/vital-youtube": "^1.0.0-rc.34",
    "lodash": "^4.17.19",
    "tailwindcss": "^3.0.23"
  }
}
