{
  "name": "@bodiless/page",
  "version": "1.0.0-rc.37",
  "description": "BodilessJS page operations",
  "author": "Guilherme Zeni <gzeni@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "/doc",
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p build:lib",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-rc.37",
    "@bodiless/components": "^1.0.0-rc.37",
    "@bodiless/core": "^1.0.0-rc.37",
    "@bodiless/data": "^1.0.0-rc.37",
    "@bodiless/fclasses": "^1.0.0-rc.37",
    "@bodiless/ui": "^1.0.0-rc.37",
    "axios": "^0.21.0",
    "informed": "^4.44.0",
    "lodash": "^4.17.19",
    "sleep-promise": "^8.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
