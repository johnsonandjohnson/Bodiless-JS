{
  "name": "@canvasx/gatsby-plugin-akamai-auth",
  "version": "0.3.7",
  "description": "A Gatsby plugin to enable authentication using Akamai Identity Cloud/Janarain.",
  "author": "Ali Al Mustafa <aalmusta@its.jnj.com>",
  "homepage": "",
  "license": "UNLICENSED",
  "files": [
    "src/",
    "dist",
    "index.js",
    "index.d.ts",
    "gatsby-browser.js",
    "gatsby-node.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://sourcecode.jnj.com/scm/asx-nbfp/canvasx.git"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run copy:janrain:css && npm run copy:janrain:html && npm run theme:init",
    "build:required": "npm run build",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf -rf \"dist\" && rimraf index*",
    "copy:janrain:css": "copyfiles -f \"./src/janrain/styles/*.css\" \"./dist/janrain/styles/\"",
    "copy:janrain:html": "copyfiles -f \"./src/janrain/index.html\" \"./dist/janrain/\"",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "theme:init": "echo $null >> index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "copyfiles": "^2.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "session-file-store": "^1.3.1",
    "typescript": "^4.0.0"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^16"
  }
}
