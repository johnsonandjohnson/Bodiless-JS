    "@bodiless/richtext": "^0.0.36",
    "@bodiless/richtext-ui": "^0.0.36",
    "lodash": "^4.17.11",
    "nuka-carousel": "^4.4.7",
    "rc-menu": "^8.0.0-alpha.5"
  },
  "peerDependencies": {
    "react": "^16.8.4"
  }
