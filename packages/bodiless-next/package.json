{
  "name": "@bodiless/next",
  "version": "1.0.0-rc.37",
  "description": "Bodiless integration with NextJS.",
  "author": "Marco Pagliarulo <mpaglia1@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "build-css.js",
    "bodiless.next.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p copy build:lib",
    "build:lib": "tsc -b ./tsconfig.json ./tsconfig.cjs.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,png}\" \"./lib/\" && copyfiles \"bodiless.next.config.js\" \"./lib/\""
  },
  "dependencies": {
    "@bodiless/core": "^1.0.0-rc.37",
    "@bodiless/core-ui": "^1.0.0-rc.37",
    "@bodiless/components": "^1.0.0-rc.37",
    "@bodiless/components-ui": "^1.0.0-rc.37",
    "@bodiless/data": "^1.0.0-rc.37",
    "@bodiless/fclasses": "^1.0.0-rc.37",
    "@bodiless/git": "^1.0.0-rc.37",
    "@bodiless/page": "^1.0.0-rc.37",
    "@bodiless/webpack": "^1.0.0-rc.37",
    "isomorphic-git": "^1.10.1",
    "find-up": "^4.1.0",
    "next": "^13.3.4",
    "lodash": "^4.17.19",
    "plaiceholder": "^2.5.0",
    "webpack": "^5.69.1",
    "react-helmet": "^6.1.0",
    "glob": "^7.1.6",
    "autoprefixer": "^10.4.2",
    "dotenv": "^8.2.0",
    "tailwindcss": "^3.0.23",
    "css-loader": "^5.2.7",
    "style-loader": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "generate-robotstxt": "^8.0.3",
    "sitemap": "^7.1.1",
    "minimatch": "^3.0.4",
    "fast-glob": "^3.2.5",
    "node-cache": "^5.1.2",
    "postcss-url": "^10.1.3",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@types/uuid": "^3.4.4",
    "@types/react-helmet": "^6.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
