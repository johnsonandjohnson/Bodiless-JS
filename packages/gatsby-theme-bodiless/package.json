{
  "name": "@bodiless/gatsby-theme-bodiless",
  "version": "0.2.8",
  "description": "A Gatsby theme for BodilessJS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "bin": {
    "generate-env-vars": "./bin/generate-env-vars",
    "bl-validate-logs": "./bin/bl-validate-logs",
    "bl-clear-logs": "./bin/bl-clear-logs"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "/bin",
    "dist",
    "index.js",
    "index.d.ts",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "queries.jsx",
    "Logger.js",
    "src/no-scroll-settings.json",
    "build-css.js",
    "create-node.js",
    "tailwind.base.config.js",
    "/doc",
    "bodiless.content.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api --mode file",
    "clean": "rimraf -rf \"dist\" && rimraf index* && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/dist/**/*.{json,jpg,png}\" \".\""
  },
  "dependencies": {
    "@bodiless/accordion": "^0.2.8",
    "@bodiless/card": "^0.2.8",
    "@bodiless/components": "^0.2.8",
    "@bodiless/core": "^0.2.8",
    "@bodiless/core-ui": "^0.2.8",
    "@bodiless/fclasses": "^0.2.8",
    "@bodiless/layouts": "^0.2.8",
    "@bodiless/navigation": "^0.2.8",
    "@bodiless/organisms": "^0.2.8",
    "@bodiless/ui": "^0.2.8",
    "@types/nodegit": "^0.26.11",
    "@types/walk": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.0",
    "crypto-browserify": "^3.12.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.5",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-emotion": "^2.0.6",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "4.8.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "informed": "~3.13.1",
    "lodash": "^4.17.19",
    "md5-file": "^5.0.0",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-lite": "^1.3.1",
    "nodegit": "^0.27.0",
    "null-loader": "^3.0.0",
    "postcss": "^8.2.4",
    "process": "^0.11.10",
    "slash": "^2.0.0",
    "sleep-promise": "^8.0.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^2.0.2",
    "universal-cookie": "^4.0.3",
    "walk": "^2.3.14",
    "walkdir": "^0.4.1"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
