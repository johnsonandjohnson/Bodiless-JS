{
  "name": "bodiless-js",
  "description": "Bodiless-JS",
  "version": "1.0.0-rc.1",
  "keywords": [
    "gatsby"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "bootstrap": "pnpm run timestamp && pnpm run clean:symlinks && pnpm install --shamefully-hoist && pnpm run timestamp",
    "bootstrap:gatsby-cloud": "pnpm i && npx lerna bootstrap",
    "build": "cross-env NODE_ENV=production pnpm run -r build",
    "build:only": "cross-env NODE_ENV=production pnpm run -r build",
    "build:packages": "cross-env NODE_ENV=production pnpm run -r --filter './packages/**' build",
    "build:watch": "cross-env NODE_ENV=development pnpm run -r build:watch --parallel",
    "build:env-vars": "cross-env NODE_ENV=production pnpm run -r build:env-vars --stream",
    "build:tailwind-configs": "cross-env NODE_ENV=production pnpm run -r build:tailwind-configs --stream",
    "build:docs": "cross-env NODE_ENV=production pnpm run -r build:docs --stream",
    "build:api-doc": "cross-env NODE_ENV=production pnpm run -r build:api-doc --stream",
    "check": "cross-env NODE_ENV=production pnpm run check --stream",
    "clean": "pnpm run clean && rimraf .eslintcache sites/*/.env.development sites/*/.env.production",
    "clean:symlinks": "node ./scripts/remove-site-symlinks @bodiless",
    "cy:run": "cypress run --browser chrome --record --key 0dcfb624-e152-453a-9375-aa710adb57ab",
    "cy:open": "cypress open",
    "cypress": "run-p -r start cy:open",
    "copyright": "node ./scripts/add-copyright",
    "fix": "eslint --fix  --cache --ext .js,.jsx,.ts,.tsx packages sites",
    "fix:target": "eslint --fix  --cache --ext .js,.jsx,.ts,.tsx",
    "fix:change": "git diff --name-only --diff-filter=d master... | grep -e 'tsx$' -e 'jsx$' -e 'js$' -e 'ts$' | xargs -L 1 pnpm run fix:target",
    "fresh": "pnpm run clean && pnpm clean -y && rimraf node_modules && pnpm run setup",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx packages sites --",
    "lint:target": "eslint --cache --ext .js,.jsx,.ts,.tsx",
    "lint:change": "git diff --name-only --diff-filter=d master... | grep -e 'tsx$' -e 'jsx$' -e 'js$' -e 'ts$' | xargs -L 1 pnpm run lint:target",
    "new": "node ./scripts/new-starter",
    "publish:all": "pnpm run build && lerna publish --registry $NPM_REGISTRY",
    "publish:from-package": "pnpm run build && lerna publish from-package --registry $NPM_REGISTRY",
    "publish:patch": "lerna version patch --conventional-commits --force-publish",
    "publish:minor": "lerna version minor --conventional-commits",
    "publish:major": "lerna version major --conventional-commits",
    "lerna:publish": "lerna publish from-git --yes --dist-tag latest",
    "lerna:canary": "lerna publish --canary --include-merged-tags --preid ks-canary --dist-tag ks-canary --force-publish",
    "preinstall": "npx only-allow pnpm",
    "pw:run": "npx playwright test playwright/tests/smoke/* --project=chromium --workers 1",
    "psh:setup-starter": "node ./scripts/psh-setup-starter",
    "serve": "pnpm run serve --scope @sites/${APP_SITE_NAME:-test-site} --stream",
    "setup": "pnpm run bootstrap && pnpm run build:packages && pnpm run timestamp",
    "setup:gatsby-cloud": "npm run bootstrap:gatsby-cloud && pnpm run build:packages",
    "start": "pnpm run start --stream --scope @sites/${APP_SITE_NAME:-test-site}",
    "sync:fix": "syncpack fix-mismatches",
    "sync:check": "syncpack list-mismatches",
    "test": "jest",
    "test:monorepo-pw": "WAIT_ON_TIMEOUT=600000 start-server-and-test sites:launch-monorepo http://localhost:8005 pw:run",
    "test:ci": "WAIT_ON_TIMEOUT=600000 start-server-and-test sites:launch-test http://localhost:8005 cy:run",
    "test:monorepo": "WAIT_ON_TIMEOUT=600000 start-server-and-test sites:launch-monorepo http://localhost:8005 cy:run",
    "test:watch": "jest --watch",
    "timestamp": "echo timestamp=$(date \"+%Y/%m/%d %H:%M:%S\")"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run sync:check && pnpm run lint"
    }
  },
  "dependencies": {
    "@types/html2canvas": "0.0.35",
    "complexity-report": "^2.0.0-alpha",
    "cypress": "^4.11.0",
    "cypress-failed-log": "^2.6.0",
    "debug": "^4.1.1",
    "html2canvas": "^1.0.0-rc.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "start-server-and-test": "^1.12.0",
    "symlink-dir": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/runtime": "^7.18.0",
    "@playwright/test": "1.22",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.158",
    "@types/rc-tooltip": "^3.7.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.2",
    "@types/slate": "^0.47.9",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "axios": "^0.21.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^1.4.0",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.19.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "ora": "^4.0.2",
    "pnpm": "^7.14.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^2.6.3",
    "syncpack": "^6.2.0",
    "ts-jest": "^26.4.4",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typedoc": "^0.22.6",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "engines": {
    "node": ">=16.9"
  }
}
